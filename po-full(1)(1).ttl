# ------ 流程命名空间 ------
@prefix ex-abc: <http://asiainfo.com/ex-abc#> .
@prefix ex-abc: <http://asiainfo.com/ex-abc#> .
@prefix ex-abc: <http://asiainfo.com/ex-abc#> .
@prefix ex-abc:  <http://asiainfo.com/ex-abc#> .
@prefix ex-abc:   <http://asiainfo.com/ex-abc-map#> .

# *********************************************
# 0. 继承 template 基类（禁止改动）
# *********************************************
ex-abc:Entity a owl:Class ; rdfs:subClassOf prov:Entity ; rdfs:label "实体"@zh .
ex-abc:Object a owl:Class ; rdfs:subClassOf ex-abc:Entity ; rdfs:label "本体对象"@zh .
ex-abc:Action a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "行动"@zh .
ex-abc:Function a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "决策/逻辑函数"@zh .

ex-abc:isPrimaryKey a owl:AnnotationProperty ; rdfs:label "是否主键"@zh .
ex-abc:isTitle a owl:AnnotationProperty ; rdfs:label "是否标题"@zh .
ex-abc:domainAttribute a owl:AnnotationProperty ; rdfs:label "域属性"@zh .
ex-abc:rangeAttribute a owl:AnnotationProperty ; rdfs:label "值域属性"@zh .

ex-abc:bindAction a owl:ObjectProperty ; rdfs:domain ex-abc:Object ; rdfs:range ex-abc:Action ; rdfs:label "本体对象关联行动"@zh .
ex-abc:bindFunction a owl:ObjectProperty ; rdfs:domain ex-abc:Object ; rdfs:range ex-abc:Function ; rdfs:label "本体对象关联函数"@zh .
ex-abc:custom_relation a owl:ObjectProperty ; rdfs:label "自定义对象与对象之间的关系"@zh .

# *********************************************
# 1. 实体定义
# *********************************************
ex-abc:PurchaseOrder a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "采购订单"@zh .
ex-abc:PurchaseOrderLine a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "采购订单行"@zh .
ex-abc:ShipmentLine a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "采购订单发运行"@zh .
ex-abc:DistributionLine a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "采购订单分配行"@zh .
ex-abc:BusinessRole      a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "业务角色"@zh .
ex-abc:BusinessDocument  a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "业务单据"@zh .
ex-abc:BusinessEvent     a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "业务事件"@zh .
ex-abc:BusinessStatus    a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "业务状态"@zh .
ex-abc:SystemInterface   a owl:Class ; rdfs:subClassOf ex-abc:Object ; rdfs:label "系统接口"@zh .

# *********************************************
# 2. 采购订单建立流程 – 全部接口字段
