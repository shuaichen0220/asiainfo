ex-abc:BUDGET_CONTRACT_NUM   a owl:DatatypeProperty ; rdfs:domain ex-abc:DistributionLine ; rdfs:range xsd:string .
ex-abc:BUDGET_CONTRACT_LINE_NUM a owl:DatatypeProperty ; rdfs:domain ex-abc:DistributionLine ; rdfs:range xsd:string .

# *********************************************
# 4. 采购订单撤销流程 – 全部接口字段
# *********************************************
ex-abc:ACTION   a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
# 其余字段与变更流程语义相同，直接复用 ex-abc: 命名空间下的同名属性，无需重复声明。

# *********************************************
# 5. 采购订单关闭流程 – 全部接口字段
# *********************************************
ex-abc:ACTION    a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
# 其余字段与变更/撤销流程语义相同，直接复用已有属性。

# *********************************************
# 6. SCM ↔ ERP 字段级等价映射（核心）
# *********************************************
ex-abc:PO_LINE_ID                    a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:PO_LINE_NUM .
ex-abc:EXPENDITURE_ORGANIZATION_ID   a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:EXPENDITURE_ORG_ID .
ex-abc:ORG_ID_ERP                    a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:ORG_ID .
ex-abc:PROJECT_ID                    a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:PROJECT_NUMBER .
ex-abc:AGENT_ID                      a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:AGENT_NUM .
ex-abc:TAX_AMOUNT                    a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:TAX_SUM .
ex-abc:UNIT_PRICE_ERP                a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:UNIT_PRICE .
ex-abc:EXPENDITURE_ITEM_DATE_ERP     a owl:DatatypeProperty ; owl:equivalentProperty ex-abc:EXPENDITURE_ITEM_DATE .

# *********************************************
# 7. 个体定义
# *********************************************
ex-abc:Role_Purchaser   a ex-abc:BusinessRole ; rdfs:label "采购员"@zh .
ex-abc:Role_Requester   a ex-abc:BusinessRole ; rdfs:label "需求部门申请人"@zh .
ex-abc:Role_Warehouse   a ex-abc:BusinessRole ; rdfs:label "仓储部门"@zh .
ex-abc:Doc_PurchaseRequisition  a ex-abc:BusinessDocument ; dcterms:identifier "ACC.SC.01.01-1" ; rdfs:label "采购申请单"@zh .
ex-abc:Doc_Contract             a ex-abc:BusinessDocument ; dcterms:identifier "ACC.SC.01.01-2" ; rdfs:label "采购合同"@zh .
ex-abc:Doc_GRN                  a ex-abc:BusinessDocument ; dcterms:identifier "ACC.SC.01.03-1" ; rdfs:label "入库单"@zh .
ex-abc:Doc_ReturnNotice         a ex-abc:BusinessDocument ; dcterms:identifier "ACC.SC.01.03-2" ; rdfs:label "退货出库通知单"@zh .
ex-abc:Event_CreateOrder      a ex-abc:BusinessEvent ; rdfs:label "创建订单事件"@zh .
ex-abc:EventangeOrder      a ex-abc:BusinessEvent ; rdfs:label "变更订单事件"@zh .
ex-abc:Event_CancelOrder      a ex-abc:BusinessEvent ; rdfs:label "撤销订单事件"@zh .
ex-abc:Event_CloseOrder       a ex-abc:BusinessEvent ; rdfs:label "关闭订单事件"@zh .
ex-abc:Event_ApproveOrder     a ex-abc:BusinessEvent ; rdfs:label "审批订单事件"@zh .
ex-abc:Event_SyncERP          a ex-abc:BusinessEvent ; rdfs:label "同步ERP事件"@zh .
ex-abc:Event_ReturnToVendor   a ex-abc:BusinessEvent ; rdfs:label "退货至供应商事件"@zh .
ex-abc:Status_New       a ex-abc:BusinessStatus ; rdfs:label "新建"@zh .
ex-abc:Status_Approved  a ex-abc:BusinessStatus ; rdfs:label "已审批"@zh .
ex-abc:Status_Received  a ex-abc:BusinessStatus ; rdfs:label "已接收"@zh .
ex-abc:Status_InStock   a ex-abc:BusinessStatus ; rdfs:label "已入库"@zh .
ex-abc:Status_OutStock  a ex-abc:BusinessStatus ; rdfs:label "已出库"@zh .
ex-abc:Status_Cancelled a ex-abc:BusinessStatus ; rdfs:label "已撤销"@zh .
ex-abc:Status_Closed    a ex-abc:BusinessStatus ; rdfs:label "已关闭"@zh .
ex-abc:IF_00001 a ex-abc:SystemInterface ; dcterms:identifier "OSB_ERP_PO_HQ_00001" ; rdfs:label "采购订单创建接口"@zh .
ex-abc:IF_00012 a ex-abc:SystemInterface ; dcterms:identifier "OSB_ERP_PO_HQ_00012" ; rdfs:label "采购订单修改/取消/关闭接口"@zh .


# *********************************************
# 8. 对象属性（关系）
# *********************************************
ex-abc:hasLine          a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range ex-abc:PurchaseOrderLine ; rdfs:label "拥有订单行"@zh .
ex-abc:hasShipment      a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range ex-abc:ShipmentLine ; rdfs:label "拥有发运行"@zh .
ex-abc:hasDistribution  a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range ex-abc:DistributionLine ; rdfs:label "拥有分配行"@zh .
ex-abc:lineOf          owl:inverseOf ex-abc:hasLine .
ex-abc:shipmentOf      owl:inverseOf ex-abc:hasShipment .
ex-abc:distributionOf  owl:inverseOf ex-abc:hasDistribution .
ex-abc:hasRole           a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range ex-abc:BusinessRole ; rdfs:label "拥有角色"@zh .
ex-abc:hasDocument       a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range ex-abc:BusinessDocument ; rdfs:label "拥有单据"@zh .
ex-abc:hasEvent          a owl:ObjectProperty ; rdfs:domain ex-abc:Object ; rdfs:range ex-abc:BusinessEvent ; rdfs:label "拥有事件"@zh .
ex-abc:hasStatus         a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range ex-abc:BusinessStatus ; rdfs:label "拥有状态"@zh .
ex-abc:triggers          a owl:ObjectProperty ; rdfs:domain ex-abc:BusinessEvent ; rdfs:range ex-abc:BusinessEvent ; rdfs:label "触发事件"@zh .
ex-abc:generatedBy       a owl:ObjectProperty ; rdfs:domain ex-abc:BusinessDocument ; rdfs:range ex-abc:BusinessEvent ; rdfs:label "由事件生成"@zh .
ex-abc:validatedBy       a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range ex-abc:Function ; rdfs:label "由函数校验"@zh .
ex-abc:synchronizedBy    a owl:ObjectProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range ex-abc:SystemInterface ; rdfs:label "由接口同步"@zh .

# *********************************************
# 9. 流程溯源 Annotation
# *********************************************
ex-abc:PurchaseOrder  dcterms:source "ACC.SC.01.01" ; dcterms:description "采购订单建立流程主实体"@zh .
ex-abc:ACTION         dcterms:source "ACC.SC.01.02" .
ex-abc:ACTION         dcterms:source "ACC.SC.01.03" .
ex-abc:ACTION          dcterms:source "ACC.SC.01.04" .
ex-abc:BusinessEvent  rdfs:subClassOf prov:Activity .
ex-abc:BusinessDocument  rdfs:subClassOf prov:Entity .
ex-abc:PurchaseOrder  rdfs:subClassOf prov:Entity .
ex-abc:Event_CreateOrder
    prov:used ex-abc:Doc_PurchaseRequisition ;
    prov:wasAssociatedWith ex-abc:Role_Purchaser ;
    prov:generated ex-abc:PurchaseOrder .
ex-abc:Event_SyncERP
    prov:used ex-abc:PurchaseOrder ;
    prov:wasAssociatedWith ex-abc:IF_00001 .

# *********************************************
# 10. 业务规则
# *********************************************
ex-abc:FREE_FLAG
    rdfs:comment "订单关闭时，若已接收数量 > 0，则 FREE_FLAG 不可更新" .
ex-abc:PO_AMOUNT
    rdfs:comment "变更后的订单含税金额必须 ≤ 合同可用含税金额" .
ex-abc:ACTION
    rdfs:comment "撤销订单时，若已入库且已出库，必须先触发退库事件" .
ex-abc:allowedAction  a owl:DatatypeProperty ; rdfs:domain ex-abc:SystemInterface ; rdfs:range xsd:string .
ex-abc:IF_00012
    rdfs:comment "接口 00012 仅支持 ACTION ∈ {U,C,S,O}" ;
    ex-abc:allowedAction "U,C,S,O" .
ex-abc:SinglePO  a  ex-abc:PurchaseOrder ;
    dcterms:identifier "EXAMPLE-PO-0001" ;
    ex-abc:hasStatus  ex-abc:Status_New ;
    ex-abc:synchronizedBy  ex-abc:IF_00001 ;
    ex-abc:hasRole  ex-abc:Role_Purchaser .
ex-abc:Event_CreateOrder
    prov:used            ex-abc:Doc_PurchaseRequisition ;
    prov:wasAssociatedWith ex-abc:Role_Purchaser ;
    prov:generated       ex-abc:SinglePO .
ex-abc:Event_SyncERP
    prov:used            ex-abc:SinglePO ;
    prov:wasAssociatedWith ex-abc:IF_00001 .

