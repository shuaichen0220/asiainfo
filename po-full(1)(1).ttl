ex-abc:ITEM_MADE_BY          a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:ITEM_MODLE_CODE       a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:ITEM_TYPE             a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:PRD_NUM               a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:FA_LISTS              a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
