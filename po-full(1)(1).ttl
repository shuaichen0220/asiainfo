ex-abc:CONTACT_NAME          a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:CONTACT_EMAIL         a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:DESCRIPTION           a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:AGENT_NUM             a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:TAX_SUM               a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:decimal .
