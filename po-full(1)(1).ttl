# *********************************************
# 3. 采购订单变更流程 – 全部接口字段
# *********************************************
ex-abc:DO_FLAG            a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:ACTION             a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:AGENT_NUM          a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:DESCRIPTION        a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:TAX_SUM            a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:decimal .
ex-abc:PO_AMOUNT          a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:decimal .
ex-abc:FREE_FLAG          a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .
ex-abc:EXP_TYPE           a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrder ; rdfs:range xsd:string .

# 行级
ex-abc:ACTION_LINE        a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:PO_LINE_NUM     a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:integer .
ex-abc:ITEM_DESCRIPTION a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:QUANTITY        a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:decimal .
ex-abc:UNIT_PRICE      a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:decimal .
ex-abc:TAX_CODE        a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:string .
ex-abc:LINE_TAX        a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:decimal .
ex-abc:LINE_AMT_TAX    a owl:DatatypeProperty ; rdfs:domain ex-abc:PurchaseOrderLine ; rdfs:range xsd:decimal .

# 发运行
