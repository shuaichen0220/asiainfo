ex-abc:DESTINATION_TYPE      a owl:DatatypeProperty ; rdfs:domain ex-abc:DistributionLine ; rdfs:range xsd:string .
ex-abc:COST_CENTER_NUM       a owl:DatatypeProperty ; rdfs:domain ex-abc:DistributionLine ; rdfs:range xsd:string .
