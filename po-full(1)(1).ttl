ex-abc:LINE_LOCATION_NUM     a owl:DatatypeProperty ; rdfs:domain ex-abc:ShipmentLine ; rdfs:range xsd:integer .
ex-abc:ORGANIZATION_ID       a owl:DatatypeProperty ; rdfs:domain ex-abc:ShipmentLine ; rdfs:range xsd:integer .
