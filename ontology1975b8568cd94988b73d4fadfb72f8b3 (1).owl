@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://asiainfo.com/example-owl#> .

##################################
# 基础抽象类与通用语义（禁止变动）
##################################
ex:Entity a owl:Class ; rdfs:subClassOf prov:Entity ; rdfs:label "实体"@zh .
ex:Object a owl:Class ; rdfs:subClassOf ex:Entity ; rdfs:label "本体对象"@zh .
ex:Action a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "行动"@zh .
ex:Function a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "决策/逻辑函数"@zh .
ex:isPrimaryKey a owl:AnnotationProperty ; rdfs:label "是否主键"@zh .
ex:bindAction a owl:ObjectProperty ; rdfs:label "本体对象关联行动"@zh ; rdfs:domain ex:Object ; rdfs:range ex:Action .
ex:bindFunction a owl:ObjectProperty ; rdfs:label "本体对象关联函数"@zh ; rdfs:domain ex:Object ; rdfs:range ex:Function .
ex:custom_relation a owl:ObjectProperty ; rdfs:label "自定义对象与对象之间的关系"@zh .
ex:Parameter a owl:Class ; rdfs:label "函数参数"@zh .
ex:hasParameter a owl:ObjectProperty ; rdfs:label "有参数"@zh ; rdfs:domain ex:Function ; rdfs:range ex:Parameter .
ex:paramName a owl:DatatypeProperty ; rdfs:label "参数名"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:paramType a owl:DatatypeProperty ; rdfs:label "参数类型"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:paramDescription a owl:DatatypeProperty ; rdfs:label "参数描述"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:isOptional a owl:DatatypeProperty ; rdfs:label "是否可选"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:boolean .
ex:defaultValue a owl:DatatypeProperty ; rdfs:label "默认值"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:ontologyName a owl:Class ; rdfs:label "本体名称"@zh .

##################################
# 本体名称
##################################
ex:CSAT_customer_satisfaction a ex:ontologyName ;
  rdfs:label "客户满意度原因分析本体"@zh .

##################################
# 可用函数
##################################
ex:get_prediction a ex:Function ;
  rdfs:label "调用预测模型获取满意度预测结果"@zh ;
  rdfs:comment """调用满意度预测模型，获取满意度得分预测以及TOP3预测指标（技术指标）和技术指标所对应的业务指标。
    业务指标是根据业务经验映射得到的可量化指标，用于反映产品体验状态。预测模型返回的业务指标对应的客户产品使用满意度中相关的属性。
    需要根据属性的具体值，结合业务指标，来判断哪些issue_type是导致客户满意度低的潜在原因。"""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "prod_inst_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] .

ex:get_solution a ex:Function ;
  rdfs:label "根据问题类型获取修复策略"@zh ;
  rdfs:comment "当判断出导致客户满意度低的issue_type为哪些后，调用该函数来为这些issue_type获取相应的修复策略"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "issue_types" ; ex:paramType "list" ; ex:isOptional false ] .

ex:insert_new_plan a ex:Function ;
  rdfs:label "插入生成的建议方案"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "content" ; ex:paramType "xsd:string" ; ex:isOptional false ], [ a ex:Parameter ; ex:paramName "cust_id" ; ex:paramType "xsd:string" ; ex:isOptional false ], [ a ex:Parameter ; ex:paramName "prod_inst_id" ; ex:paramType "xsd:string" ; ex:isOptional false ], [ a ex:Parameter ; ex:paramName "used_stra_ids" ; ex:paramType "xsd:string" ; ex:isOptional false ] .

ex:is_low_satisfaction a ex:Function ;
  rdfs:label "判断是否属于低满意度"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "score" ; ex:paramType "xsd:double" ; ex:isOptional false ] .

ex:simulate_satisfaction_improvement a ex:Function ;
  rdfs:label "模拟仿真可提升的满意度数值"@zh ;
  rdfs:comment """Mock simulation logic to estimate satisfaction improvement for a given strategy.
        Returns a random value in [1.0, 3.0] with one decimal place."""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "strategy_text" ; ex:paramType "xsd:string" ; ex:isOptional false ] .

ex:test a ex:Function ;
  rdfs:label "test"@zh ;
  rdfs:comment "@function_label: 测试"@zh  .

##################################
# Action动作定义
##################################
ex:createCustomer a ex:Action ;
  rdfs:label "新增客户"@zh  .

ex:Customer ex:bindAction ex:createCustomer .

