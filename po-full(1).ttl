exabc:SITE_NUMBER           a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:DEMAND_DATE           a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:date .
exabc:ITEM_MADE_BY          a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:ITEM_MODLE_CODE       a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:ITEM_TYPE             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:PRD_NUM               a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:FA_LISTS              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:TAX_FLAG              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:TAX_CODE              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:LINE_TAX              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
exabc:LINE_AMT_TAX          a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
exabc:LINE_DESC             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:INPUT_EXT_LINE        a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .

# 2.3 Shipment
exabc:LINE_LOCATION_NUM     a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:integer .
exabc:ORGANIZATION_ID       a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:integer .
exabc:PROMISED_DATE         a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:date .
exabc:DEMAND_DATE_SHIP      a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:date .
exabc:EXPAND_ASSET_NUMBER   a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:string .
exabc:INPUT_EXT_SHIP        a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:string .

# 2.4 Distribution
exabc:DISTRIBUTION_NUM      a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:integer .
exabc:DESTINATION_TYPE      a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:COST_CENTER_NUM       a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:PROJECT_NUMBER        a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:TASK_NUMBER           a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:EXPENDITURE_TYPE      a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:EXPENDITURE_ORG_ID    a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:integer .
exabc:EXPENDITURE_ITEM_DATE a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:date .
exabc:EXPENSE_ACCOUNT       a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:DIMENSION_CODE        a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:BUDGET_ID             a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:BUDGET_CONTRACT_NUM   a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:BUDGET_CONTRACT_LINE_NUM a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:INPUT_EXT_DIST        a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .

# *********************************************
# 3. 采购订单变更流程 – 全部接口字段
# *********************************************
exabc:DO_FLAG            a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:ACTION             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:AGENT_NUM          a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:DESCRIPTION        a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:TAX_SUM            a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:decimal .
exabc:PO_AMOUNT          a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:decimal .
exabc:FREE_FLAG          a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:EXP_TYPE           a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .

# 行级
exabc:ACTION_LINE        a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:PO_LINE_NUM     a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:integer .
exabc:ITEM_DESCRIPTION a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:QUANTITY        a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
exabc:UNIT_PRICE      a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
exabc:TAX_CODE        a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:LINE_TAX        a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
exabc:LINE_AMT_TAX    a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .

# 发运行
exabc:PROMISED_DATE   a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:date .
exabc:DEMAND_DATE     a owl:DatatypeProperty ; rdfs:domain exabc:ShipmentLine ; rdfs:range xsd:date .

# 分配行
exabc:DIMENSION_CODE  a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:BUDGET_ID       a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:BUDGET_CONTRACT_NUM   a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .
exabc:BUDGET_CONTRACT_LINE_NUM a owl:DatatypeProperty ; rdfs:domain exabc:DistributionLine ; rdfs:range xsd:string .

# *********************************************
# 4. 采购订单撤销流程 – 全部接口字段
# *********************************************
exabc:ACTION   a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
# 其余字段与变更流程语义相同，直接复用 exabc: 命名空间下的同名属性，无需重复声明。

# *********************************************
# 5. 采购订单关闭流程 – 全部接口字段
# *********************************************
exabc:ACTION    a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
# 其余字段与变更/撤销流程语义相同，直接复用已有属性。

# *********************************************
# 6. SCM ↔ ERP 字段级等价映射（核心）
# *********************************************
exabc:PO_LINE_ID                    a owl:DatatypeProperty ; owl:equivalentProperty exabc:PO_LINE_NUM .
exabc:EXPENDITURE_ORGANIZATION_ID   a owl:DatatypeProperty ; owl:equivalentProperty exabc:EXPENDITURE_ORG_ID .
exabc:ORG_ID_ERP                    a owl:DatatypeProperty ; owl:equivalentProperty exabc:ORG_ID .
exabc:PROJECT_ID                    a owl:DatatypeProperty ; owl:equivalentProperty exabc:PROJECT_NUMBER .
exabc:AGENT_ID                      a owl:DatatypeProperty ; owl:equivalentProperty exabc:AGENT_NUM .
exabc:TAX_AMOUNT                    a owl:DatatypeProperty ; owl:equivalentProperty exabc:TAX_SUM .
exabc:UNIT_PRICE_ERP                a owl:DatatypeProperty ; owl:equivalentProperty exabc:UNIT_PRICE .
exabc:EXPENDITURE_ITEM_DATE_ERP     a owl:DatatypeProperty ; owl:equivalentProperty exabc:EXPENDITURE_ITEM_DATE .

# *********************************************
# 7. 个体定义
# *********************************************
exabc:Role_Purchaser   a exabc:BusinessRole ; rdfs:label "采购员"@zh .
exabc:Role_Requester   a exabc:BusinessRole ; rdfs:label "需求部门申请人"@zh .
exabc:Role_Warehouse   a exabc:BusinessRole ; rdfs:label "仓储部门"@zh .
exabc:Doc_PurchaseRequisition  a exabc:BusinessDocument ; dcterms:identifier "ACC.SC.01.01-1" ; rdfs:label "采购申请单"@zh .
exabc:Doc_Contract             a exabc:BusinessDocument ; dcterms:identifier "ACC.SC.01.01-2" ; rdfs:label "采购合同"@zh .
exabc:Doc_GRN                  a exabc:BusinessDocument ; dcterms:identifier "ACC.SC.01.03-1" ; rdfs:label "入库单"@zh .
exabc:Doc_ReturnNotice         a exabc:BusinessDocument ; dcterms:identifier "ACC.SC.01.03-2" ; rdfs:label "退货出库通知单"@zh .
exabc:Event_CreateOrder      a exabc:BusinessEvent ; rdfs:label "创建订单事件"@zh .
exabc:EventangeOrder      a exabc:BusinessEvent ; rdfs:label "变更订单事件"@zh .
exabc:Event_CancelOrder      a exabc:BusinessEvent ; rdfs:label "撤销订单事件"@zh .
exabc:Event_CloseOrder       a exabc:BusinessEvent ; rdfs:label "关闭订单事件"@zh .
exabc:Event_ApproveOrder     a exabc:BusinessEvent ; rdfs:label "审批订单事件"@zh .
exabc:Event_SyncERP          a exabc:BusinessEvent ; rdfs:label "同步ERP事件"@zh .
exabc:Event_ReturnToVendor   a exabc:BusinessEvent ; rdfs:label "退货至供应商事件"@zh .
exabc:Status_New       a exabc:BusinessStatus ; rdfs:label "新建"@zh .
exabc:Status_Approved  a exabc:BusinessStatus ; rdfs:label "已审批"@zh .
exabc:Status_Received  a exabc:BusinessStatus ; rdfs:label "已接收"@zh .
exabc:Status_InStock   a exabc:BusinessStatus ; rdfs:label "已入库"@zh .
exabc:Status_OutStock  a exabc:BusinessStatus ; rdfs:label "已出库"@zh .
exabc:Status_Cancelled a exabc:BusinessStatus ; rdfs:label "已撤销"@zh .
exabc:Status_Closed    a exabc:BusinessStatus ; rdfs:label "已关闭"@zh .
exabc:IF_00001 a exabc:SystemInterface ; dcterms:identifier "OSB_ERP_PO_HQ_00001" ; rdfs:label "采购订单创建接口"@zh .
exabc:IF_00012 a exabc:SystemInterface ; dcterms:identifier "OSB_ERP_PO_HQ_00012" ; rdfs:label "采购订单修改/取消/关闭接口"@zh .


# *********************************************
# 8. 对象属性（关系）
# *********************************************
exabc:hasLine          a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range exabc:PurchaseOrderLine ; rdfs:label "拥有订单行"@zh .
exabc:hasShipment      a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range exabc:ShipmentLine ; rdfs:label "拥有发运行"@zh .
exabc:hasDistribution  a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range exabc:DistributionLine ; rdfs:label "拥有分配行"@zh .
exabc:lineOf          owl:inverseOf exabc:hasLine .
exabc:shipmentOf      owl:inverseOf exabc:hasShipment .
exabc:distributionOf  owl:inverseOf exabc:hasDistribution .
exabc:hasRole           a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range exabc:BusinessRole ; rdfs:label "拥有角色"@zh .
exabc:hasDocument       a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range exabc:BusinessDocument ; rdfs:label "拥有单据"@zh .
exabc:hasEvent          a owl:ObjectProperty ; rdfs:domain exabc:Object ; rdfs:range exabc:BusinessEvent ; rdfs:label "拥有事件"@zh .
exabc:hasStatus         a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range exabc:BusinessStatus ; rdfs:label "拥有状态"@zh .
exabc:triggers          a owl:ObjectProperty ; rdfs:domain exabc:BusinessEvent ; rdfs:range exabc:BusinessEvent ; rdfs:label "触发事件"@zh .
exabc:generatedBy       a owl:ObjectProperty ; rdfs:domain exabc:BusinessDocument ; rdfs:range exabc:BusinessEvent ; rdfs:label "由事件生成"@zh .
exabc:validatedBy       a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range exabc:Function ; rdfs:label "由函数校验"@zh .
exabc:synchronizedBy    a owl:ObjectProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range exabc:SystemInterface ; rdfs:label "由接口同步"@zh .

# *********************************************
# 9. 流程溯源 Annotation
# *********************************************
exabc:PurchaseOrder  dcterms:source "ACC.SC.01.01" ; dcterms:description "采购订单建立流程主实体"@zh .
exabc:ACTION         dcterms:source "ACC.SC.01.02" .
exabc:ACTION         dcterms:source "ACC.SC.01.03" .
exabc:ACTION          dcterms:source "ACC.SC.01.04" .
exabc:BusinessEvent  rdfs:subClassOf prov:Activity .
exabc:BusinessDocument  rdfs:subClassOf prov:Entity .
exabc:PurchaseOrder  rdfs:subClassOf prov:Entity .
exabc:Event_CreateOrder
    prov:used exabc:Doc_PurchaseRequisition ;
    prov:wasAssociatedWith exabc:Role_Purchaser ;
    prov:generated exabc:PurchaseOrder .
exabc:Event_SyncERP
    prov:used exabc:PurchaseOrder ;
    prov:wasAssociatedWith exabc:IF_00001 .

# *********************************************
# 10. 业务规则
# *********************************************
exabc:FREE_FLAG
    rdfs:comment "订单关闭时，若已接收数量 > 0，则 FREE_FLAG 不可更新" .
exabc:PO_AMOUNT
    rdfs:comment "变更后的订单含税金额必须 ≤ 合同可用含税金额" .
exabc:ACTION
    rdfs:comment "撤销订单时，若已入库且已出库，必须先触发退库事件" .
exabc:allowedAction  a owl:DatatypeProperty ; rdfs:domain exabc:SystemInterface ; rdfs:range xsd:string .
exabc:IF_00012
    rdfs:comment "接口 00012 仅支持 ACTION ∈ {U,C,S,O}" ;
    exabc:allowedAction "U,C,S,O" .
exabc:SinglePO  a  exabc:PurchaseOrder ;
    dcterms:identifier "EXAMPLE-PO-0001" ;
    exabc:hasStatus  exabc:Status_New ;
    exabc:synchronizedBy  exabc:IF_00001 ;
    exabc:hasRole  exabc:Role_Purchaser .
exabc:Event_CreateOrder
    prov:used            exabc:Doc_PurchaseRequisition ;
    prov:wasAssociatedWith exabc:Role_Purchaser ;
    prov:generated       exabc:SinglePO .
exabc:Event_SyncERP
    prov:used            exabc:SinglePO ;
    prov:wasAssociatedWith exabc:IF_00001 .

