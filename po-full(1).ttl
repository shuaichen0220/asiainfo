@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix prov:     <http://www.w3.org/ns/prov#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix exabc:       <http://asiainfo.com/example-owl#> .

# ------ 流程命名空间 ------
@prefix exabc: <http://asiainfo.com/exabc#> .
@prefix exabc: <http://asiainfo.com/exabc#> .
@prefix exabc: <http://asiainfo.com/exabc#> .
@prefix exabc:  <http://asiainfo.com/exabc#> .
@prefix exabc:   <http://asiainfo.com/exabc-map#> .

# *********************************************
# 0. 继承 template 基类（禁止改动）
# *********************************************
exabc:Entity a owl:Class ; rdfs:subClassOf prov:Entity ; rdfs:label "实体"@zh .
exabc:Object a owl:Class ; rdfs:subClassOf exabc:Entity ; rdfs:label "本体对象"@zh .
exabc:Action a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "行动"@zh .
exabc:Function a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "决策/逻辑函数"@zh .

exabc:isPrimaryKey a owl:AnnotationProperty ; rdfs:label "是否主键"@zh .
exabc:isTitle a owl:AnnotationProperty ; rdfs:label "是否标题"@zh .
exabc:domainAttribute a owl:AnnotationProperty ; rdfs:label "域属性"@zh .
exabc:rangeAttribute a owl:AnnotationProperty ; rdfs:label "值域属性"@zh .

exabc:bindAction a owl:ObjectProperty ; rdfs:domain exabc:Object ; rdfs:range exabc:Action ; rdfs:label "本体对象关联行动"@zh .
exabc:bindFunction a owl:ObjectProperty ; rdfs:domain exabc:Object ; rdfs:range exabc:Function ; rdfs:label "本体对象关联函数"@zh .
exabc:custom_relation a owl:ObjectProperty ; rdfs:label "自定义对象与对象之间的关系"@zh .

# *********************************************
# 1. 实体定义
# *********************************************
exabc:PurchaseOrder a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "采购订单"@zh .
exabc:PurchaseOrderLine a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "采购订单行"@zh .
exabc:ShipmentLine a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "采购订单发运行"@zh .
exabc:DistributionLine a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "采购订单分配行"@zh .
exabc:BusinessRole      a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "业务角色"@zh .
exabc:BusinessDocument  a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "业务单据"@zh .
exabc:BusinessEvent     a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "业务事件"@zh .
exabc:BusinessStatus    a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "业务状态"@zh .
exabc:SystemInterface   a owl:Class ; rdfs:subClassOf exabc:Object ; rdfs:label "系统接口"@zh .

# *********************************************
# 2. 采购订单建立流程 – 全部接口字段
# *********************************************
# 2.1 Header
exabc:DO_FLAG               a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:PRI_KEY               a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string ; exabc:isPrimaryKey true .
exabc:ORG_ID                a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:integer .
exabc:SOR_PO_DOC            a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:VENDOR_NUMBER         a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:INNER_FLAG            a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:CONTACT_NAME          a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:CONTACT_EMAIL         a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:DESCRIPTION           a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:AGENT_NUM             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:TAX_SUM               a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:decimal .
exabc:PO_AMOUNT             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:decimal .
exabc:FREE_FLAG             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:CURRENCY_CODE         a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:RATE_TYPE             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:RATE_DATE             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:date .
exabc:CURRENCY_CONVERSION_RATE a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:decimal .
exabc:EXP_TYPE              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:PUR_TYPE              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:CON_NUM_HQ            a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:CON_NUM               a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .
exabc:INPUT_EXT             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrder ; rdfs:range xsd:string .

# 2.2 Line
exabc:PO_LINE_NUM           a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:integer .
exabc:LINE_TYPE             a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:ITEM_NUMBER           a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:ITEM_DESCRIPTION      a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:string .
exabc:QUANTITY              a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
exabc:UNIT_PRICE            a owl:DatatypeProperty ; rdfs:domain exabc:PurchaseOrderLine ; rdfs:range xsd:decimal .
